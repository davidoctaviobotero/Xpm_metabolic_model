    "from gurobipy import *\n",
    "m = Model()\n",
    "v0 = m.addVar()\n",
    "v1 = m.addVar()\n",
    "m.update()\n",
    "m.addConstr(v0 - v1 <=0)\n",
    "m.addConstr(v0 + v1 <=0)\n",
    "m.addConstr(-0.25*v0 + v1 <=1)\n",
    "m.setObjective(v1, GRB.MAXIMIZE)\n",
    "m.optimize()"
   ]
